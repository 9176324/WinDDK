TARGETNAME=TTY
TARGETTYPE=DYNLINK

DLLBASE=0x50000000

USE_MSVCRT=1
DLLENTRY=_DllMainCRTStartup

!if defined(DBG)
C_DEFINES=$(C_DEFINES) -DKERNEL_MODE -DUSERMODE_DRIVER -D_DEBUG -DDEBUG
!else
C_DEFINES=$(C_DEFINES) -DKERNEL_MODE -DUSERMODE_DRIVER
!endif

MSC_WARNING_LEVEL=/W4 /WX

TARGETLIBS=$(SDK_LIB_PATH)\kernel32.lib \
           $(SDK_LIB_PATH)\ole32.lib  \
           $(SDK_LIB_PATH)\winspool.lib  \
           $(SDK_LIB_PATH)\uuid.lib

INCLUDES=  ..\inc

SOURCES= TTYUD.cpp   \
         oemcom.cpp  \
         debug.cpp   \
         TTYUD.rc

!IF "$(BUILD_PASS)" == "PASS2" || "$(BUILD_PASS)" == "PASSALL"
NTTARGETFILES=COPYDLL
!ENDIF

# Build with Vista libs but make sure sample can still run downlevel
_NT_TARGET_VERSION=$(_NT_TARGET_VERSION_WINXP)
WIN32_WINNT_VERSION =$(LATEST_WIN32_WINNT_VERSION)
NTDDI_VERSION=$(LATEST_NTDDI_VERSION)

