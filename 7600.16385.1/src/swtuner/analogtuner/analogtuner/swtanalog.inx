[BdaEncoder.NTamd64.Services]
AddService=SWTANALOG, 0x00000002, BdaAnlgTuner.ServiceInstall

[BdaTVAudio.Filter]
AddReg = BdaTVAudio.Filter.AddReg

[BdaTVAudio.Filter.Addreg]
