;HKR,,CLSID,,%Proxy.CLSID%
HKR,,FriendlyName,,%BdaTVAudio.FilterDesc%

[BdaTVAudio.NTx86.Services]
AddService=SWTANALOG, 0x00000002, BdaAnlgTuner.ServiceInstall

[BdaTVAudio.NTamd64.Services]
AddService=SWTANALOG, 0x00000002, BdaAnlgTuner.ServiceInstall

[BdaAnlgCapture.Filter]
AddReg = BdaAnlgCapture.Filter.AddReg

[BdaAnlgCapture.Filter.Addreg]
HKR,,CLSID,,%Proxy.CLSID%
HKR,,FriendlyName,,%BdaAnlgCapture.FilterDesc%

[BdaAnlgCapture.NTx86.Services]
AddService=SWTANALOG, 0x00000002, BdaAnlgTuner.ServiceInstall

[BdaAnlgCapture.NTamd64.Services]
AddService=SWTANALOG, 0x00000002, BdaAnlgTuner.ServiceInstall


