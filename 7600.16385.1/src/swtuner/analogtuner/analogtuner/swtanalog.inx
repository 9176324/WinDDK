[Version]
signature="$Windows NT$"
Class=MEDIA
ClassGUID={4d36e96c-e325-11ce-bfc1-08002be10318}
Provider=%Msft%
DriverVer=05/22/2007,6.0.6001.16640
CatalogFile=SWTANALOG.cat

[ControlFlags]
;ExcludeFromSelect=*

[SourceDisksNames]
1 = %BDA_DISK%,"",

[SourceDisksFiles]
SWTANALOG.sys   = 1

[DestinationDirs]
SWTANALOG.CopyFiles=10,system32\drivers

[Manufacturer]
%Msft%=Microsoft,NTx86,NTamd64

[Microsoft.NTx86]
%SWTANALOGNTSC.DeviceDesc%=SWTANALOG,ms_swtNTSC
%SWTANALOGPAL.DeviceDesc%=SWTANALOG,ms_swtPAL

[Microsoft.NTamd64]
%SWTANALOGNTSC.DeviceDesc%=SWTANALOG,ms_swtNTSC
%SWTANALOGPAL.DeviceDesc%=SWTANALOG,ms_swtPAL

[SWTANALOG.NTx86]
Include     = ks.inf, kscaptur.inf, bda.inf
needs=KS.Registration, KSCAPTUR.Registration, BDA.Installation.NT
CopyInf=SWTANALOG.inf
CopyFiles=SWTANALOG.CopyFiles

[SWTANALOG.NTamd64]
Include     = ks.inf, kscaptur.inf, bda.inf
needs=KS.Registration, KSCAPTUR.Registration, BDA.Installation.NT
CopyInf=SWTANALOG.inf
CopyFiles=SWTANALOG.CopyFiles

;---------------------------------------------------------------
;                I n t e r f a c e s
;---------------------------------------------------------------

[SWTANALOG.NTx86.Interfaces]
AddInterface = %KSCATEGORY_TVTUNER%, %KSSTRING_BdaAnlgSWTunerFilter%, BdaAnlgTuner.Filter,0
AddInterface = %KSCATEGORY_CROSSBAR%, %KSSTRING_BdaXBarFilter%, BdaXbar.Filter
AddInterface = %KSCATEGORY_ENCODER%, %KSSTRING_BdaEncoderFilter%, BdaEncoder.Filter
AddInterface = %KSCATEGORY_MULTIPLEXER%, %KSSTRING_BdaEncoderFilter%, BdaEncoder.Filter
AddInterface = %KSCATEGORY_TVAUDIO%, %KSSTRING_BdaTVAudioFilter%, BdaTVAudio.Filter
AddInterface = %KSCATEGORY_CAPTURE%, %KSSTRING_BdaAnlgCaptureFilter%, BdaAnlgCapture.Filter
AddInterface = %KSCATEGORY_VIDEO%, %KSSTRING_BdaAnlgCaptureFilter%, BdaAnlgCapture.Filter

[SWTANALOG.NTamd64.Interfaces]
AddInterface = %KSCATEGORY_TVTUNER%, %KSSTRING_BdaAnlgSWTunerFilter%, BdaAnlgTuner.Filter,0
AddInterface = %KSCATEGORY_CROSSBAR%, %KSSTRING_BdaXBarFilter%, BdaXbar.Filter
AddInterface = %KSCATEGORY_ENCODER%, %KSSTRING_BdaEncoderFilter%, BdaEncoder.Filter
AddInterface = %KSCATEGORY_MULTIPLEXER%, %KSSTRING_BdaEncoderFilter%, BdaEncoder.Filter
AddInterface = %KSCATEGORY_TVAUDIO%, %KSSTRING_BdaTVAudioFilter%, BdaTVAudio.Filter
AddInterface = %KSCATEGORY_CAPTURE%, %KSSTRING_BdaAnlgCaptureFilter%, BdaAnlgCapture.Filter
AddInterface = %KSCATEGORY_VIDEO%, %KSSTRING_BdaAnlgCaptureFilter%, BdaAnlgCapture.Filter

[TunerInterface.NT]
AddReg=SWTANALOG.Reader.AddReg



;---------------------------------------------------------------
;                A d d R e g
;---------------------------------------------------------------
[SWTANALOG.Reader.AddReg]
;HKR,,DevLoader,,*NTKERN
;HKR,,NTMPDriver,,SWTANALOG.sys
HKR,,CLSID,,%KSTvTune.CLSID%
HKR,,FriendlyName,,%SWTANALOG.FriendlyName%

[SWTANALOG.CopyFiles]
SWTANALOG.sys

[SWTANALOG.NTx86.Services]
AddService=SWTANALOG, 0x00000002, SWTANALOG.ServiceInstall

[SWTANALOG.NTamd64.Services]
AddService=SWTANALOG, 0x00000002, SWTANALOG.ServiceInstall

[SWTANALOG.ServiceInstall]
DisplayName=%SWTANALOG.DeviceDesc%
ServiceType=%SERVICE_KERNEL_DRIVER%
StartType=%SERVICE_DEMAND_START%
ErrorControl=%SERVICE_ERROR_NORMAL%
ServiceBinary=%10%\system32\drivers\SWTANALOG.sys
LoadOrderGroup=ExtendedBase


[BdaAnlgTuner.Filter]
AddReg = BdaAnlgTuner.Filter.AddReg, PinNames.AddReg, Category.Addreg

[BdaAnlgTuner.Filter.AddReg]
HKR,,CLSID,,%KSTvTune.CLSID%
;HKR,,CLSID,,%Proxy.CLSID%
HKR,,FriendlyName,,%BdaAnlgTuner.FilterDesc%

[BdaAnlgTuner.NTx86.Services]
AddService=SWTANALOG, 0x00000002, BdaAnlgTuner.ServiceInstall

[BdaAnlgTuner.NTamd64.Services]
AddService=SWTANALOG, 0x00000002, BdaAnlgTuner.ServiceInstall

[BdaAnlgTuner.ServiceInstall]
