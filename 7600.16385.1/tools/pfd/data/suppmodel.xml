    <External>
      <ExternalID type="param" index="0"/>
      <PreConditions>
        <IsWritableString value="never"/>
        <IsNullOk value="never"/>
        <RequiresZeroTermination value="always"/>
        <IsValidPointer value="always"/>
      </PreConditions>
      <PostConditions/>
    </External>
    <External>
      <ExternalID type="retval"/>
      <PostConditions>
        <MustBeChecked value="always"/>
      </PostConditions>
    </External>
  </Function>

