#include "ntddk_slic.h"

DbgBreakPoint.entry
{
        abort "$fname should only be called when the driver is DEBUG build";
}

DbgBreakPointWithStatus.entry
{
        abort "$fname should only be called when the driver is DEBUG build";
}
