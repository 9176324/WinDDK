#include "ntddk_slic.h"


sdv_MINIPORT_PAUSE.exit
{  
	if ( !NDIS_SUCCESS($return) && !NDIS_PENDING($return))
	{
    abort "MiniportPause cannot return a value different from SUCCESS or PENDING.";
  }
}

