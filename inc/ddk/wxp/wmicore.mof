[Dynamic, Provider("WMIProv"),
 WMI,
 Description("ACPI Table data") : amended,
 guid("{5DAF38AE-F6F8-4d90-8199-EBDE6800EC3B}")
]
class MSAcpiInfo
{
    [key, read]
     string InstanceName;
    [read] boolean Active;


